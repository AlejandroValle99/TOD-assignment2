<%- include('partials/header') %>

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Users</title>
  </head>
  <body>
    <main>
      <% messages.forEach(message => { %>
      <ul>
        <%- include('user', {user:message.user, text: message.text, added
        :message.added}) %>
      </ul>
      <% }) %>
    </main>
  </body>
</html>
